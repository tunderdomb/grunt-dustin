{! scoping !}
{#global}

  {! variable !} {! (nested) partial !}
  hey {ho} let's {>"nested/partial/go"/}{~n}

  {! empty list !}
  {?emptylist}
    {#emptylist}
      {.}
    {/emptylist}
  {:else}
    I hope nobody's taping this!
  {/emptylist}

  {! list !}
  {#stringlist}
    {.|s}{~n}
  {/stringlist}

{/global}

{! inheritance !}
{>"layouts/extended"/}
{<title}
  Child Title
{/title}
{<content}
  Child Content
{/content}

{! macros !}
{~n}
{@macro hey="hey"}
  {hey} ho let's go
{/macro}
{~n}
{@macro partial="go" go="ho"/}
{~n}
{@macro partial="a" title="hello"/}

{! raw !}
{~n}
{`
{! raw !}
Preserve all new lines,
{a}
whitespace, and braces
`}